import random
import string

print("Welcome  To Password Generator ")

length = int(input('\nEnter The Length Of Password: '))

lower = string.ascii_lowercase
upper = string.ascii_uppercase
num = string.digits
symbols = "!@#$%^&*"

all = lower + upper + num + symbols

temp = random.sample(all,length)

password = "".join(temp)

print(password)
